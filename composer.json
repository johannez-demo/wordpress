{
    "name": "johannez/demo-wp",
    "description": "Demo site for Wordpress",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/advanced-custom-fields": "^6.3",
        "wpackagist-plugin/regenerate-thumbnails": "^3.1",
        "wpackagist-plugin/user-role-editor": "^4.6",
        "wpackagist-plugin/wp-redis": "^1.0",
        "wp-cli/wp-cli-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Demo\\": "src/Demo"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
